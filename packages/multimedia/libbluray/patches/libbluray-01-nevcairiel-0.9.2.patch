-
-    for (ii = 0; ii < pl->list_count; ii++) {
-        MPLS_PI *pi;
-
-        pi = &pl->play_item[ii];
-        // Ignore titles with repeated segments
-        if (strcmp(pi->clip[0].clip_id, m2ts) == 0) {
-            count++;
-        }
-    }
-    return count;
-}
