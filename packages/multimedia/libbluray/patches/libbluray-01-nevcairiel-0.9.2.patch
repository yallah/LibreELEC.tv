+#include <stdio.h>
+#include <stdlib.h>
+#include <unistd.h>
+
+#include "libbluray/bluray.h"
+
+#if defined(_WIN32)
+#include <windows.h>
+#define sleep(x) Sleep(x)
+#endif
+
+static void _usage(void) {
+    printf("Usage: [path to disc] [starting object]\n");
+}
+
+int main(int argc, char** argv)
+{
+    if (argc < 3) {
+        _usage();
+        return 0;
+    }
+
+    printf("%s %s\n", argv[1], argv[2]);
