+            break;
+
+        case 2:
+        case 4:
+            indent_printf(level, "SubPath Id: %02x", ss->subpath_id);
+            indent_printf(level, "SubClip Id: %02x", ss->subclip_id);
+            indent_printf(level, "PID: %04x", ss->pid);
