-static int
-_filter_repeats(MPLS_PL *pl, int repeats)
-{
-    int ii;
-
-    for (ii = 0; ii < pl->list_count; ii++) {
-        MPLS_PI *pi;
-
-        pi = &pl->play_item[ii];
-        // Ignore titles with repeated segments
-        if (_find_repeats(pl, pi->clip[0].clip_id) > repeats) {
-            return 0;
-        }
-    }
-    return 1;
-}
